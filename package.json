{
  "name": "moviapp-api",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "gen:types": "yarn strapi ts:generate-types",
    "prepare": "husky install",
    "ci": "yarn install --production --frozen-lockfile",
    "lint": "yarn check-types && yarn eslint && yarn prettier:check",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "eslint": "eslint --cache --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
    "eslint:fix": "yarn eslint --fix",
    "prettier:check": "prettier --check '**/*.ts' '**/*.js' '**/*.tsx'",
    "prettier:fix": "prettier --write '**/*.ts' '**/*.js' '**/*.tsx'",
    "check-types": "tsc --noemit",
    "doc": "speccy resolve -i -q -o documentation/full_documentation.yaml documentation/openapi.yaml",
    "do:init": "doctl apps create --spec spec.yaml --context moviapp",
    "do:deploy": "doctl apps create-deployment $APP_ID --context moviapp",
    "do:update-spec": "doctl apps update $APP_ID --spec spec.yaml --context moviapp",
    "do:logs": "doctl apps logs $APP_ID --context moviapp",
    "do:deployments": "doctl apps list-deployments $APP_ID --context moviapp",
    "cp:locales": "copyfiles src/locales/*.yaml dist/"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/async": "^3.2.20",
    "@types/js-yaml": "^4.0.5",
    "@types/koa": "^2.13.6",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.8.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "fixjson": "^1.1.2",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^13.2.2",
    "nunjucks": "^3.2.4",
    "prettier": "^2.8.8",
    "speccy": "^0.11.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.31",
    "@strapi/plugin-i18n": "4.11.0",
    "@strapi/plugin-users-permissions": "4.11.0",
    "@strapi/provider-upload-aws-s3": "^4.11.0",
    "@strapi/strapi": "4.11.0",
    "async": "^3.2.4",
    "lodash.camelcase": "^4.3.0",
    "pg": "8.11.0",
    "pg-connection-string": "^2.6.0",
    "pluralize": "^8.0.0",
    "slug": "^8.2.2",
    "strapi-plugin-config-sync": "^1.1.2",
    "strapi-plugin-io": "^1.1.4",
    "strapi-plugin-transformer": "3.1.0"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "38e1dfaf-d20a-4134-9140-dd9d85dfb04a"
  },
  "engines": {
    "node": "16.19.1",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
